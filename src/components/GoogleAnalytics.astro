---
const gtagId = import.meta.env.VITE_GOOGLE_TAG_ID;
---

<script
  is:inline
  type="text/partytown"
  async
  src=`https://www.googletagmanager.com/gtag/js?id=${gtagId}`></script>
<script is:inline type="text/partytown">
  window.dataLayer = window.dataLayer || [];
  function gtag() {
    dataLayer.push(arguments);
  }
  gtag("js", new Date());

  gtag("config", "${gtagId}");
</script>
